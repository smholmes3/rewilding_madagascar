    "import pandas as pd\n",
    "\n",
    "# Load CSV\n",
    "df = pd.read_csv(\"allfiles_for_predicting.csv\")\n",
    "\n",
    "# Replace the prefix\n",
    "df[\"file\"] = df[\"file\"].str.replace(\n",
    "    \"/mnt/d/\", \n",
    "    \"/Volumes/Expansion/\", \n",
    "    regex=False\n",
    ")\n",
    "\n",
    "# Save back to CSV\n",
    "df.to_csv(\"allfiles_for_predicting_mac.csv\", index=False)"
   ]
  }
